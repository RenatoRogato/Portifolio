    {
      "name": "EngUnits_VIRT",
      "tagType": "Folder",
      "tags": [
        {
          "valueSource": "expr",
          "expression": "(({[.]kW}/{[.]kVA}))",
          "dataType": "Float4",
          "historyProvider": "UMS_DB",
          "alarms": [
            {
              "mode": "AboveValue",
              "setpointA": 1.0,
              "name": "PF \u003e 1",
              "label": "Power Factor above limit",
              "priority": "High"
            },
            {
              "mode": "BelowValue",
              "name": "PF \u003c 0",
              "label": "Power Factor below limits",
              "priority": "High"
            }
          ],
          "name": "FP",
          "historyEnabled": true,
          "tagType": "AtomicTag"
        },
        {
          "valueSource": "expr",
          "expression": "({[.]TEST_V_480}*{[.]RANDON_I})",
          "historyProvider": "UMS_DB",
          "name": "W",
          "historyEnabled": false,
          "tagType": "AtomicTag"
        },
        {
          "valueSource": "expr",
          "expression": "sin(-36.9)*{[.]kVA}",
          "historyProvider": "UMS_DB",
          "name": "kVAr",
          "historyEnabled": false,
          "tagType": "AtomicTag"
        },
        {
          "valueSource": "expr",
          "expression": "{[System]Gateway/Alarming/Clear and Acked}+{[System]Gateway/Alarming/Clear and Unacked}+{[.]Active Alarms}",
          "historyProvider": "UMS_DB",
          "sourceTagPath": "[.]Active Alarms",
          "name": "All Alarms",
          "historyEnabled": true,
          "tagType": "AtomicTag"
        },
        {
          "valueSource": "expr",
          "expression": "(sqrt(3)*{[.]TEST_V_480}*{[.]RANDON_I})/1000",
          "historyProvider": "UMS_DB",
          "name": "kVA",
          "historyEnabled": false,
          "tagType": "AtomicTag"
        },
        {
          "valueSource": "expr",
          "expression": "abs(sin(now()*2)*40+100)",
          "historyProvider": "UMS_DB",
          "name": "RANDON_I",
          "historyEnabled": false,
          "tagType": "AtomicTag",
          "sampleMode": "OnChange"
        },
        {
          "valueSource": "memory",
          "historyProvider": "UMS_DB",
          "alarms": [
            {
              "mode": "BelowValue",
              "setpointA": 473.0,
              "anyChange": true,
              "timeOnDelaySeconds": 60.0,
              "name": "(V) \u003c 10%",
              "label": "voltage (V) below 10% from limit value ",
              "priority": "Medium",
              "ackNotesReqd": false
            },
            {
              "mode": "AboveValue",
              "setpointA": 12.0,
              "anyChange": true,
              "timeOnDelaySeconds": 60.0,
              "name": "(V) \u003d 0",
              "label": "voltage (V) \u003d 0V"
            },
            {
              "mode": "AboveValue",
              "setpointA": 528.0,
              "anyChange": true,
              "timeOnDelaySeconds": 60.0,
              "name": "(V) \u003e 10%",
              "label": "voltage (V) above 10% from limit value",
              "priority": "Medium",
              "displayPath": ""
            }
          ],
          "name": "TEST_V_480",
          "historyEnabled": true,
          "value": 480,
          "tagType": "AtomicTag"
        },
        {
          "valueSource": "expr",
          "expression": "{[System]Gateway/Alarming/Active and Acked}+{[System]Gateway/Alarming/Active and Unacked}",
          "historyProvider": "UMS_DB",
          "name": "Active Alarms",
          "historyEnabled": true,
          "tagType": "AtomicTag"
        },
        {
          "valueSource": "expr",
          "expression": "{[.]W}/1000",
          "historyProvider": "UMS_DB",
          "name": "kW",
          "historyEnabled": false,
          "tagType": "AtomicTag"
        }
      ]
    },
